<p id="notice"><%= notice %></p>

<h1>Listing Pcos</h1>

<table>
  <thead>
    <tr>
      <th>PCO# </th>
      <th>Type </th>
      <th>Description </th>
      <th>Proposal# </th>
      <th>Al# </th>
      <th>DDC CO# </th>
      <th>DF Submitted </th>
      <th>DF Returned </th>
      <th>Proposed </th>
      <th>TSTV Estimate </th>
      <th>GT Estimate </th>
      <th>Submitted </th>
      <th>DDC Approved </th>
      <% if current_user.admin? %>
      <th colspan="3"></th>
      <% end %>
    </tr>
  </thead>

  <tbody>
    <% @pcos.each do |pco| %>
      <tr>
        <td><%= pco.num %></td>
        <td><%= pco.pco_type %></td>
        <td><%= pco.description %></td>
        <td><%= pco.sub_proposal_num %></td>
        <td><%= pco.al_num %></td>
        <td><%= pco.ddc_co_num %></td>
        <td><%= pco.df_submitted %></td>
        <td><%= pco.df_returned %></td>
        <td><%= pco.proposed %></td>
        <td><%= pco.tstv_estimate %></td>
        <td><%= pco.gt_estimate %></td>
        <td><%= pco.submitted %></td>
        <td><%= pco.ddc_approved %></td>
        <% if current_user.admin? %>
        <td><%= link_to 'Show', pco %></td>
        <td><%= link_to 'Edit', edit_pco_path(pco) %></td>
        <td><%= link_to 'Destroy', pco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pco', new_pco_path %>
